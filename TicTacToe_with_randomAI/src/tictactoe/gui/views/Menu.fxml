<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="400.0" styleClass="pane" stylesheets="@../css/Main.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.gui.controller.MenuController">
   <children>
      <Label layoutX="22.0" layoutY="99.0" prefHeight="82.0" prefWidth="364.0" text="Tic Tac Toe" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="55.0" />
         </font>
      </Label>
      <Button fx:id="single" layoutX="64.0" layoutY="260.0" mnemonicParsing="false" onAction="#SinglePlayer" prefHeight="40.0" prefWidth="280.0" styleClass="menu_buttons" text="Single Player">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Button>
      <Button fx:id="multi" layoutX="64.0" layoutY="360.0" mnemonicParsing="false" onAction="#MultiPlayer" prefHeight="40.0" prefWidth="280.0" styleClass="menu_buttons" text="Multiplayer">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
